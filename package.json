{
  "name": "s3up-meta",
  "version": "1.0.25",
  "description": "A simple function to upload files to Amazon S3. Refactored from s3up by Lepozepo and added metadata for Amazon caching of uploaded files.",
  "main": "client/index.js",
  "dependencies": {
    "aws-sdk": "^2.610.0",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.19",
    "fibers": "^4.0.2",
    "lodash.assignin": "^4.2.0",
    "lodash.every": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isstring": "^4.0.1",
    "lodash.last": "^3.0.0",
    "lodash.noop": "^3.0.1",
    "lodash.reduce": "^4.6.0",
    "lodash.times": "^4.3.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "coffeelint": "^2.1.0",
    "coffeescript": "^2.5.0"
  },
  "maintainers": [
    {
      "name": "paulincai",
      "email": "paulailincai@icloud.com"
    }
  ],
  "scripts": {
    "make-docs": "npm run prepublish && jsdoc lib -r -d docs && rm -rf lib",
    "postpublish": "rm -rf jsFromCoffee && rm -rf client && rm -rf server",
    "prepublish": "coffee --no-header --compile --output jsFromCoffee/ src/ && node_modules/.bin/babel jsFromCoffee -d ./",
    "test": "coffeelint -f coffeelint.json ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activitree/s3up-meta"
  },
  "readme": "A simple function to upload files to Amazon S3. Refactored from s3up by Lepozepo and added metadata for caching of uploaded files.",
  "readmeFilename": "README.md",
  "keywords": [
    "s3",
    "upload",
    "multipart",
    "meteor"
  ],
  "author": "paulincai",
  "license": "MIT",
  "homepage": "https://github.com/activitree/s3up-meta"
}
